{
  "name": "spl-client",
  "version": "0.1.0",
  "description": "spl client",
  "main": "electron/main.js",
  "author": {
    "name": "waneon",
    "email": "waneon.kim@gmail.com"
  },
  "scripts": {
    "react-start": "webpack serve --mode=development",
    "start": "concurrently \"yarn react-start\" \"wait-on http://localhost:8000 && electron .\"",
    "react-build": "webpack --mode=production",
    "build": "yarn react-build && electron-builder --publish=never"
  },
  "build": {
    "appId": "waneon.spl",
    "productName": "spl-client",
    "files": [
      ".env",
      "electron",
      "dist/index.html",
      "dist/main.js",
      "!node_modules",
      "node_modules/dotenv"
    ],
    "linux": {
      "target": "AppImage"
    },
    "win": {
      "target": "nsis"
    }
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "antd": "^4.19.4",
    "dotenv": "^16.0.0",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.2",
    "recoil": "^0.6.1",
    "socket.io-client": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-react": "^7.16.7",
    "babel-loader": "^8.2.4",
    "concurrently": "^7.0.0",
    "css-loader": "^6.7.1",
    "electron": "^17.2.0",
    "electron-builder": "^22.14.13",
    "html-webpack-plugin": "^5.5.0",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "wait-on": "^6.0.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  }
}
